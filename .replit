modules = ["python-3.11"]
[agent]
expertMode = true
integrations = ["web_scraper==1.0.0"]

[nix]
channel = "stable-25_05"
packages = ["xcodebuild", "zlib"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "telegram_bot"

[[workflows.workflow]]
name = "telegram_bot"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "TELEGRAM_BOT_TOKEN=${{ secrets.TELEGRAM_BOT_TOKEN }} python main.py"

[workflows.workflow.metadata]
outputType = "console"
